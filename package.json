{
  "name": "open-weather-app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"json-server --watch ./src/assets/mocks/data.json\" \"ng serve --proxy-config proxy.config.json\"",
    "cy:run": "concurrently \"json-server --watch ./src/assets/mocks/data.json\" \"ng serve --proxy-config proxy.config.json\" \"cypress open\"",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:prod:deploy": "ng build --prod && firebase deploy",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.2",
    "@angular/cdk": "^11.1.2",
    "@angular/common": "~11.1.2",
    "@angular/compiler": "~11.1.2",
    "@angular/core": "~11.1.2",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.1.2",
    "@angular/material": "^11.1.2",
    "@angular/platform-browser": "~11.1.2",
    "@angular/platform-browser-dynamic": "~11.1.2",
    "@angular/router": "~11.1.2",
    "@angular/service-worker": "~11.1.2",
    "codelyzer": "^6.0.1",
    "ngx-moment": "^5.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "yargs-parser": "^18.1.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.2",
    "@angular/cli": "^11.1.2",
    "@angular/compiler-cli": "~11.1.2",
    "@angular/language-service": "~11.1.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.25",
    "concurrently": "^6.0.0",
    "cypress": "6.4.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.16.1",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "moment": "^2.29.1",
    "prettier": "^2.0.5",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.1.3"
  }
