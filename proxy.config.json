{
  "/user-details/*": {
    "target": "http://localhost:3000",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true
  },
  "/openweathermap-api/*": {
    "target": "https://api.openweathermap.org/data/2.5/",
    "secure": true,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/openweathermap-api/": ""
    }
  }
}
